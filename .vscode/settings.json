{
  "typescript.tsdk": "common/temp/node_modules/typescript/lib",
  "eslint.useFlatConfig": true,
  "eslint.options": {
    "flags": [
      "unstable_ts_config"
    ]
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "yaml",
