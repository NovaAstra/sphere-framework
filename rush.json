{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.149.0",
  "pnpmVersion": "10.4.1",
  "nodeSupportedVersionRange": ">=22.11.0",
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "ensureConsistentVersions": true,
  "allowedProjectTags": [
    "eslint",
    "prettier",
    "typescript",
    "ui",
    "app"
  ],
  "projects": [
    {
      "packageName": "@sphere/eslint-config",
      "projectFolder": "configs/eslint/eslint-config",
      "tags": [
        "eslint"
      ]
    },
    {
      "packageName": "@sphere/eslint-plugin-gitignore",
      "projectFolder": "configs/eslint/eslint-plugin-gitignore",
      "tags": [
        "eslint"
      ]
    },
    {
      "packageName": "@sphere/prettier-config",
      "projectFolder": "configs/prettier-config",
      "tags": [
        "prettier"
      ]
    },
    {
      "packageName": "@sphere/typing",
      "projectFolder": "core/typing",
      "tags": [
        "typescript"
      ]
    },
    {
      "packageName": "@sphere/tsconfig",
      "projectFolder": "configs/tsconfig",
      "tags": [
        "typescript"
      ]
    },
    {
      "packageName": "@sphere/element",
      "projectFolder": "ui/element",
      "tags": [
        "ui"
      ]
    },
    {
      "packageName": "@sphere/graphix",
      "projectFolder": "ui/graphix",
      "tags": [
        "ui"
      ]
    },
    {
      "packageName": "@sphere/shared",
      "projectFolder": "core/shared"
    },
    {
      "packageName": "@sphere/widget",
      "projectFolder": "apps/widget"
    },
    {
      "packageName": "@sphere/dashboard",
      "projectFolder": "apps/dashboard"
    }
  ]
}